#include <stdio.h>

#include<string.h>

int main(){

    int t,c,i,j;

    scanf("%d", &t);

    while(t--)

    {

        char str[1000];

        int sum=0;

        scanf("%s",str);

        int l=strlen(str);

        for(i=0;i<l;i++)

        {

            if(str[i]==48||(str[i]==54)||(str[i]==57))

            sum=sum+6;

            else if(str[i]==50||(str[i]==51)||(str[i]==53))

            sum=sum+5;

            else if(str[i]==56)

            sum=sum+7;

            else if(str[i]==49)

            sum=sum+2;

            else if(str[i]==52)

            sum=sum+4;

            else if(str[i]==55)

            sum=sum+3;

        }

        if((sum%2)!=0)

        {

            printf("7");

            c=sum/2;

            for(j=0;j<c-1;j++)

            {

                printf("1");

            }

        }

        else if((sum%2)==0)

        {

            c=sum/2;

            for(j=0;j<c;j++)

            {

                printf("1");

            }

        }

        printf("\n");

    }      
}
